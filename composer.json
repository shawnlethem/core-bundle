{
    "name": "video-games-records/core-bundle",
    "description": "VideoGamesRecords CoreBundle",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Benard David",
            "email": "magicbart@gmail.com",
            "homepage": "https://www.vgr-magicbart.com/",
            "role": "Developper"
        }
    ],
    "require": {
        "php": "^8.0",
        "a2lix/translation-form-bundle": "*",
        "api-platform/api-pack": "^1.2",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "~2.0",
        "knplabs/doctrine-behaviors": "~2.0",
        "sonata-project/admin-bundle": "*",
        "sonata-project/doctrine-orm-admin-bundle": "*",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "video-games-records/dwh-bundle": "*",
        "projet-normandie/article-bundle": "*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "*",
        "symfony/framework-bundle": "^5.4"
    },
    "autoload": {
        "psr-4": { "VideoGamesRecords\\CoreBundle\\": "src/" }
    },
    "autoload-dev": {
          "psr-4": {
              "VideoGamesRecords\\CoreBundle\\Tests\\": "tests/"
          }
      },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
